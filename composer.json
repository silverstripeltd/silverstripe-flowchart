{
    "name": "chtombleson/silverstripe-flowchart",
    "description": "Flowchart silverstripe module.",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe"],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/cms": "^4",
        "silverstripe/framework": "^4"
    },
    "autoload": {
        "psr-4": {
            "ChTombleson\\Flowchart\\": "src/",
            "ChTombleson\\Flowchart\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "css",
            "js"
        ],
        "installer-name": "silverstripe-flowchart"
    },
    "scripts": {
        "lint": "phpcs -s src/ tests/",
        "lint-fix": "phpcbf -s src/ tests/",
        "test": "phpunit -d memory_limit=512M -d flush=1"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0"
    }
}
